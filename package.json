    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.1",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "node-sass": "^7.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "redbox-react": "^1.5.0",
    "resolve-url-loader": "^2.1.0",
